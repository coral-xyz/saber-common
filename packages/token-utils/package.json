{
  "name": "@coral-xyz/token-utils",
  "description": "Token-related math and transaction utilities for Solana.",
  "version": "1.15.0-beta.0",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "keywords": [
    "solana",
    "saber"
  ],
  "scripts": {
    "build": "tsc && tsc -P tsconfig.cjs.json",
    "clean": "rm -fr dist/"
  },
  "dependencies": {
    "@saberhq/solana-contrib": "1.14.11",
    "@solana/buffer-layout": "^4.0.0",
    "@solana/buffer-layout-utils": "^0.2.0",
    "@solana/spl-token": "^0.3.8",
    "@ubeswap/token-math": "^5.2.1",
    "tiny-invariant": "^1.3.1",
    "tslib": "^2.4.0"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "type": "module",
  "devDependencies": {
    "@saberhq/tsconfig": "^2.1.0",
    "@solana/web3.js": "^1.63.1",
    "@types/bn.js": "^5.1.1",
    "jsbi": "^4.3.0",
    "typescript": "^5.1.5"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.42",
    "bn.js": "^4 || ^5",
    "jsbi": "^3 || ^4"
  },
  "publishConfig": {
    "access": "public"
  }
}
